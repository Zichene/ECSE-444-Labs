 .syntax unified

 .extern test
 .global call_test

 // .section marks a new section in assembly. .text identifies it as source code;
// .rodata marks it as read-only, setting it to go in FLASH, not SRAM
.section .text.rodata

call_test:
push {lr}
mov r1, #1
mov r2, #2
bl test

pop {lr}
bx lr
